{
  "name": "fetchyourkeys",
  "version": "1.0.0",
  "description": "The official SDK for read-only interaction with the FetchYourKeys API. Easily manage your API keys securely from your backend application.",
  "main": "./lib/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "tsc && npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir ./lib-cjs && echo '{\"type\":\"commonjs\"}' > ./lib-cjs/package.json",
    "dev": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "node examples/basic.js"
  },
  "keywords": [
    "api-keys",
    "key-management",
    "security",
    "sdk",
    "fetchyourkeys",
    "api-management",
    "backend",
    "nodejs",
    "cache",
    "encryption",
    "saas",
    "developer-tools",
    "secret-management",
    "configuration",
    "environment-variables",
    "typescript",
    "javascript",
    "api-client",
    "rest-api",
    "authentication"
  ],
  "author": {
    "name": "FetchYourKeys Team",
    "email": "support@fetchyourkeys.com",
    "url": "https://fetchyourkeys.vercel.app"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "^24.7.2",
    "@types/node-cache": "^4.1.3",
    "typescript": "^5.9.3"
  }
}